@model List<Employee>

<a href="/Employees/Create" class="btn btn-outline-primary">Add</a>
<h1>All Employee</h1>


<table class="table">
    <thead>
        <tr>
            <td>ID</td>
            <td>NAME</td>
            <td>EMAIL</td>
            <td>PASSWORD</td>
            
            </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.password</td>
               
            </tr>
        }
    </tbody>
</table>